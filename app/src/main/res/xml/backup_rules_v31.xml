<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="root" path="." />
        <exclude domain="file" />
        <include domain="database" path="." />
        <include domain="sharedpref" path="." />
        <exclude domain="external" />
    </cloud-backup>
    <device-transfer>
        <include domain="root" path="." />
        <exclude domain="file" />
        <include domain="database" path="." />
        <include domain="sharedpref" path="." />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>